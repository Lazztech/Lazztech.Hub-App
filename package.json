{
  "name": "ionic",
  "version": "0.0.1",
  "author": "Ionic Framework",
  "homepage": "https://ionicframework.com/",
  "engines": {
    "node": "16.x",
    "npm": "8.x"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:dev": "ng serve",
    "start:local": "ng serve --configuration=local",
    "start:stage": "ng serve --configuration=staging",
    "start:prod": "ng serve --configuration=production",
    "start:demo": "ng serve --configuration=demo",
    "build": "ng build",
    "build:prod": "ng build --configuration production --base-href /app/",
    "prepare:ios:dev": "ionic build && npx cap sync && npx cap copy && npx cap open ios",
    "prepare:ios:stg": "ionic build --configuration=staging && npx cap sync && npx cap copy && npx cap open ios",
    "prepare:ios:prod": "ionic build --configuration production && npx cap sync && npx cap copy && npx cap open ios",
    "prepare:ios:demo": "ionic build --configuration demo && npx cap sync && npx cap copy && npx cap open ios",
    "run:ios:dev": "ionic build && npx cap sync && npx cap copy && npx cap run ios",
    "run:ios:stg": "ionic build --configuration=staging && npx cap sync && npx cap copy && npx cap run ios",
    "run:ios:prod": "ionic build --configuration production && npx cap sync && npx cap copy && npx cap run ios",
    "prepare:android:dev": "ionic build && jetify && npx cap sync && npx cap copy && npx cap open android",
    "prepare:android:stg": "ionic build --configuration=staging && jetify && npx cap sync && npx cap copy && npx cap open android",
    "prepare:android:prod": "ionic build --configuration production && jetify && npx cap sync && npx cap copy && npx cap open android",
    "run:android:dev": "ionic build && jetify && npx cap sync && npx cap copy && npx cap run android",
    "run:android:stg": "ionic build --configuration=staging && jetify && npx cap sync && npx cap copy && npx cap run android",
    "run:android:prod": "ionic build --configuration production && jetify && npx cap sync && npx cap copy && npx cap run android",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "generate": "graphql-codegen --config codegen.yml",
    "generate:watch": "graphql-codegen -w --config codegen.yml",
    "resources:ios": "cordova-res ios --skip-config --copy",
    "resources:android": "cordova-res android --skip-config --copy --icon-background-source '#FFFFFF'"
  },
  "private": true,
