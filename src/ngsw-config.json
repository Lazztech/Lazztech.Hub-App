{
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "Lazztech Hub",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/*.css",
          "/*.js",
          "/manifest.webmanifest"
        ]
      }
    }, {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**",
          "/*.(eot|svg|cur|jpg|png|webp|gif|otf|ttf|woff|woff2|ani)"
        ]
      }
    }
  ],
    "dataGroups": [
    {
      "name": "user-images",
      "urls": [
      "https://lazztech-hub-service-z84zo.ondigitalocean.app/file/**",
      "https://lazztech-hub-service-z84zo.ondigitalocean.app/avatars/**"
    ],
      "cacheConfig": {
        "strategy": "performance",
        "maxSize": 200,          
        "maxAge": "7d",         
        "timeout": "5s",  
        "cacheOpaqueResponses": true       
      }
    }
  ]
}
